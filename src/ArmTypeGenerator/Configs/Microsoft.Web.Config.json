{
  "namespaceName": "AzureDesignStudio.AzureResources",
  "usingNamespaces": [
    "AzureDesignStudio.AzureResources.Base"
  ],
  "forceOverwrite": true,
  "hintDictionary": {
    "ResourceDefinitions": [
      {
        "kind": "BaseTypeHint",
        "arguments": {
          "baseTypeNames": [
            "ResourceBase"
          ],
          "baseTypePropsToIgnore": [
            "name",
            "location",
            "tags"
          ],
          "baseTypePropsToOverride": [
            "type",
            "apiVersion"
          ]
        }
      }
    ],
    "Sites.Resources": [
      {
        "kind": "ArrayTypeHint",
        "arguments": {
          "itemsTypeName": "ResourceBase"
        }
      }
    ],
    "SitesConfig.Properties": [
      {
        "kind": "ReferenceTypeHint",
        "arguments": {
          "typeName": "object"
        }
      }
    ],
    "SitesSlotsConfig.Properties": [
      {
        "kind": "ReferenceTypeHint",
        "arguments": {
          "typeName": "object"
        }
      }
    ]
  }
}